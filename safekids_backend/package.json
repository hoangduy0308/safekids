    "test:3.5-unit": "jest tests/location-clustering.spec.js --verbose --forceExit",
    "test:3.5-integration": "jest tests/story-3.5-suggestions.integration.spec.js --verbose --forceExit",
    "test:3.5-coverage": "jest tests/location-clustering.spec.js tests/story-3.5-suggestions.integration.spec.js --coverage --forceExit",
    "test:ci": "jest --testNamePattern='Story 2|Story 3' --verbose --coverage --forceExit",
    "seed": "node src/utils/seed.js"
  },
  "keywords": [
    "parental-control",
    "child-safety",
    "gps-tracking"
  ],
  "author": "SafeKids Team",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
