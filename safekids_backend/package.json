{
  "name": "safekids-backend",
  "version": "1.0.0",
  "description": "SafeKids Backend API - Child Safety App",
  "main": "server.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --detectOpenHandles --forceExit",
    "test:local": "jest --coverage",
    "test:story-2.1": "jest --testNamePattern='Story 2.1' --verbose",
    "test:2.1-p0": "jest --testNamePattern='P0|CRITICAL' --verbose",
    "test:2.1-p1": "jest --testNamePattern='P1|HIGH' --verbose",
    "test:coverage": "jest --coverage --testNamePattern='Story 2.1'",
    "test:story-2.3": "jest tests/story-2.3.spec.js --verbose --forceExit",
    "test:2.3-p0": "jest tests/story-2.3.spec.js --testNamePattern='P0|CRITICAL' --verbose --forceExit",
    "test:2.3-p1": "jest tests/story-2.3.spec.js --testNamePattern='P1|HIGH' --verbose --forceExit",
    "test:2.3-p2": "jest tests/story-2.3.spec.js --testNamePattern='P2' --verbose --forceExit",
    "test:2.3-burn-in": "jest tests/story-2.3.spec.js --testNamePattern='BURN|burn' --verbose --forceExit",
    "test:2.3-coverage": "jest tests/story-2.3.spec.js --coverage --forceExit",
    "test:story-2.5": "jest tests/story-2.5.spec.js --verbose --forceExit",
    "test:2.5-p0": "jest tests/story-2.5.spec.js --testNamePattern='P0|CRITICAL' --verbose --forceExit",
    "test:2.5-p1": "jest tests/story-2.5.spec.js --testNamePattern='P1|HIGH' --verbose --forceExit",
    "test:2.5-p2": "jest tests/story-2.5.spec.js --testNamePattern='P2' --verbose --forceExit",
    "test:story-3.2": "jest tests/story-3.2.spec.js --verbose --forceExit",
    "test:3.2-p0": "jest tests/story-3.2.spec.js --testNamePattern='P0|CRITICAL' --verbose --forceExit",
    "test:3.2-p1": "jest tests/story-3.2.spec.js --testNamePattern='P1|HIGH' --verbose --forceExit",
    "test:3.2-p2": "jest tests/story-3.2.spec.js --testNamePattern='P2|MEDIUM' --verbose --forceExit",
    "test:3.2-coverage": "jest tests/story-3.2.spec.js --coverage --forceExit",
    "test:story-3.3": "jest tests/story-3.3.spec.js --verbose --forceExit",
    "test:3.3-p0": "jest tests/story-3.3.spec.js --testNamePattern='P0|CRITICAL' --verbose --forceExit",
    "test:3.3-p1": "jest tests/story-3.3.spec.js --testNamePattern='P1|HIGH' --verbose --forceExit",
    "test:3.3-p2": "jest tests/story-3.3.spec.js --testNamePattern='P2|MEDIUM' --verbose --forceExit",
    "test:3.3-bulk": "jest tests/story-3.3.spec.js --testNamePattern='bulk|Bulk' --verbose --forceExit",
    "test:3.3-coverage": "jest tests/story-3.3.spec.js --coverage --forceExit",
    "test:story-3.4": "jest tests/story-3.4.spec.js tests/story-3.4.stats.spec.js tests/story-3.4-automation.spec.js --verbose --forceExit",
    "test:3.4-p0": "jest tests/story-3.4*.spec.js --testNamePattern='P0|CRITICAL' --verbose --forceExit",
    "test:3.4-p1": "jest tests/story-3.4*.spec.js --testNamePattern='P1|HIGH' --verbose --forceExit",
    "test:3.4-p2": "jest tests/story-3.4*.spec.js --testNamePattern='P2' --verbose --forceExit",
    "test:3.4-coverage": "jest tests/story-3.4*.spec.js --coverage --forceExit",
    "test:geofence": "jest --testNamePattern='Geofence|geofence' --verbose --forceExit",
