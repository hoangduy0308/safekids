    "swagger-ui-express": "^5.0.1",
    "twilio": "^5.10.3"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.2.3",
    "nodemon": "^3.0.1",
    "socket.io-client": "^4.8.1",
    "supertest": "^6.3.3"
  }
}
